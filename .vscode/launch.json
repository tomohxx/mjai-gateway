{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Sample",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/sample.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "SSLKEYLOGFILE": "${workspaceFolder}/logs/pre-master-secret.log"
      }
    },
    {
      "name": "Main (Debug On)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "SSLKEYLOGFILE": "${workspaceFolder}/logs/pre-master-secret.log"
      },
      "args": ["-d"]
    },
    {
      "name": "Main (Debug Off)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "SSLKEYLOGFILE": "${workspaceFolder}/logs/pre-master-secret.log"
      }
    }
  ]
}